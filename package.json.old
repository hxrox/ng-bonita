{
    "name": "ng-bonita-build",
    "version": "1.0.0",
    "description": "Build Package @zgwnu/ng-bonita",
    "main": "index.js",
    "scripts": {
      "dist:build": "npm --prefix src/zgwnu-ng-bonita run build && npm run dist:cfg",
      "dist:rebuild": "npm --prefix src/zgwnu-ng-bonita run rebuild && npm run dist:cfg",
      "dist:cfg": "cp cfg/npm/* dist",
      "dist:publish": "npm publish dist --access public",
      "build": "npm run dist:build && npm run dist:publish",
      "rebuild": "npm run dist:rebuild && npm run dist:publish",
      "test": "echo test"
    },
    "repository": {
      "type": "git",
      "url": "git+https://github.com/zgwnu/ng-bonita.git"
    },
    "keywords": [
      "build",
      "bonita",
      "bpm",
      "zgwnu"
    ],
