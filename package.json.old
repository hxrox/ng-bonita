{
  "name": "@zgwnu/test-ng-bonita",
  "version": "1.0.0",
  "description": "Test App for ZGW NU Angular Bonita BPM Rest Api Services Package",
  "scripts": {
    "jit:cfg": "cp cfg/jit/*.* dest",
    "jit:build": "tsc -p src/",
    "jit:watch": "tsc -p src/ -w",
    "jit:serve": "lite-server -c=bs-config.json",
    "prestart": "npm run jit:cfg && npm run jit:build",
    "start": "concurrently \"npm run jit:watch\" \"npm run jit:serve\"",
    "lint": "tslint ./src/**/*.ts -t verbose"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "^4.3.6",
    "@angular/compiler": "^4.3.6",
    "@angular/core": "^4.3.6",
    "@angular/http": "^4.3.6",
    "@angular/platform-browser": "^4.3.6",
    "@angular/platform-browser-dynamic": "^4.3.6",
    "@angular/router": "^4.3.6",
    "core-js": "^2.5.1",
    "rxjs": "5.4.3",
    "systemjs": "0.20.18",
    "zone.js": "^0.8.17"
  },
  "devDependencies": {
    "concurrently": "^3.5.0",
    "lite-server": "^2.3.0",
    "typescript": "^2.5.2",
    "@types/node": "^6.0.46"
  },
  "repository": {}
}
